{
  "expo": {
    "name": "Eazyy",
    "slug": "eazyy-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.eazyy.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them."
        }
      ]
    ],
    "sdkVersion": "53.0.0",
    "platforms": ["ios", "android", "web"]
  }
}